	journal = {Journal of Nonlinear Science},
	pages = {1435--1461},
	pdf = {https://arxiv.org/pdf/1512.03795.pdf},
	selected = {false},
	title = {A Variational Model for Dislocations at Semi-coherent Interfaces},
