	journal = {ESAIM: Mathematical Modelling and Numerical Analysis},
	number = {6},
	pages = {2351--2382},
	pdf = {https://arxiv.org/pdf/1901.10162.pdf},
	selected = {true},
	title = {An optimal transport approach for solving dynamic inverse problems in spaces of measures},
