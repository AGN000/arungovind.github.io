@article{copeland-halperinDoesTimingPostoperative2020,
	title = {Does the {Timing} of {Postoperative} {Showering} {Impact} {Infection} {Rates}? {A} {Systematic} {Review} and {Meta}-{Analysis}},
	volume = {0},
	issn = {1748-6815, 1878-0539},
	shorttitle = {Does the {Timing} of {Postoperative} {Showering} {Impact} {Infection} {Rates}?},
	url = {https://www.jprasurg.com/article/S1748-6815(20)30066-8/abstract},
	doi = {10.1016/j.bjps.2020.02.007},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Introduction{\textless}/h3{\textgreater}{\textless}p{\textgreater}Optimum timing of postoperative showering varies. Earlier showering improves patient satisfaction, but the impact of the timing of showering on postoperative infection is unclear. We conducted a systematic literature review and meta-analysis to investigate outcomes of various postoperative showering practices.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}We searched PubMed to identify relevant human clinical studies in English, and searched these for additional references. Articles were reviewed for patient demographics, surgical specialty and procedure, wound closure method, placement of drains, showering protocol, and rates of infection and complications. Only randomized controlled trials were analyzed. A random effects meta-analysis model was used to determine overall infection and complication rates between patients allowed to shower within the first 48 hours postoperatively or later.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}357 studies, of which 7 and 5 were included in the infection and complications rate meta-analyses, respectively. 1,881 and 958 patients were included in each analysis; 463 patients were allowed to shower on or before postoperative day 2 ("early"), while the remainder were prohibited from showering until postoperative day 3 to beyond one week ("delayed") postoperatively. There was no difference in infection (\textit{p=}0.45, [-0.0052, 2*0.007 95\%CI]) or complication rate (\textit{p=}0.36, [-0.0046, 2*0.005 95\%CI]) with earlier vs. delayed showering protocols.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusion{\textless}/h3{\textgreater}{\textless}p{\textgreater}Published literature demonstrates no increase in the overall rate of wound infections or complications when patients showered earlier in the postoperative period. Additional randomized studies are needed to determine the ideal time for postoperative showering. These data should be considered by surgeons when determining when to permit patients to shower after surgery.{\textless}/p{\textgreater}},
	language = {English},
	number = {0},
	urldate = {2020-03-17},
	journal = {Journal of Plastic, Reconstructive \& Aesthetic Surgery},
	author = {Copeland-Halperin, Libby R. and Rada, Maria L. Reategui Via y and Levy, Joshua and Shank, Nina and Funderburk, Christopher D. and Shin, Joseph H.},
	month = feb,
	year = {2020},
	note = {Publisher: Elsevier},
	file = {Snapshot:/Users/joshualevy/Zotero/storage/R92JJVI4/fulltext.html:text/html},
}

