	urldate = {2020-01-19},
	journal = {bioRxiv},
	author = {Levy, Joshua J. and Jackson, Christopher R. and Sriharan, Aravindhan and Christensen, Brock and Vaickus, Louis J.},
	month = jan,
	year = {2020},
	pages = {2020.01.07.897801},
	file = {Full Text PDF:/Users/joshualevy/Zotero/storage/F7F8B43P/Levy et al. - 2020 - Preliminary Evaluation of the Utility of Deep Gene.pdf:application/pdf;Snapshot:/Users/joshualevy/Zotero/storage/JLB8M9J4/2020.01.07.html:text/html},
