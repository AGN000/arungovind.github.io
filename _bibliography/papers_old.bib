@article{levyLargeScaleInternalValidation2020,
	title = {A {Large}-{Scale} {Internal} {Validation} {Study} of {Unsupervised} {Virtual} {Trichrome} {Staining} {Technologies} on {Non}-alcoholic {Steatohepatitis} {Liver} {Biopsies}},
	copyright = {© 2020, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
	url = {https://www.biorxiv.org/content/10.1101/2020.07.03.187237v1},
	doi = {10.1101/2020.07.03.187237},
	abstract = {{\textless}p{\textgreater}Non-alcoholic steatohepatitis (NASH) is a type of fatty liver disease characterized by accumulation of fat in hepatocytes and concurrent inflammation. It is associated with significant morbidity as it can progress to cirrhosis and liver failure and increases the risk of developing hepatocellular carcinoma. NASH has become increasingly prevalent in the US and is a leading cause of liver transplantation for end stage liver disease and a significant public health burden. Grading and staging NASH involves liver core biopsy, followed by microscopic examination of histologic preparations of the tissue. Tissue sections are stained with hematoxylin and eosin (H\&amp;E) to grade NASH activity, and stained with trichrome to help stage fibrosis. Methods to computationally transform one stain into another on digital whole slide images (WSI) can reduce the need for additional physical staining besides H\&amp;E, alleviating personnel, equipment, and time costs. Recent computational techniques have shown promise and success for virtual staining of tissue. We conducted a large-scale validation study of the viability of virtual staining technologies for H\&amp;E to trichrome conversion on WSI (n=574). Our results demonstrate that virtual trichrome technologies present an immediate software solution that can be readily employed in the clinical setting as a diagnostic decision aid.{\textless}/p{\textgreater}},
	language = {en},
	urldate = {2020-07-23},
	journal = {bioRxiv},
	author = {Levy, Joshua J. and Azizgolshani, Nasim and Andersen, Michael J. and Suriawinata, Arief and Liu, Xiaoying and Lisovsky, Mikhail and Ren, Bing and Christensen, Brock C. and Vaickus, Louis J.},
	month = jul,
	year = {2020},
	note = {Publisher: Cold Spring Harbor Laboratory
Section: New Results},
	pages = {2020.07.03.187237},
	file = {Full Text PDF:/Users/joshualevy/Zotero/storage/I9BYLBES/Levy et al. - 2020 - A Large-Scale Internal Validation Study of Unsuper.pdf:application/pdf;Snapshot:/Users/joshualevy/Zotero/storage/DCDYT5JD/2020.07.03.html:text/html},
