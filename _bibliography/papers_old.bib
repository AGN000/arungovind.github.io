@article{GordonGradualPolyploidGenome2020,
	title = {Gradual polyploid genome evolution revealed by pan-genomic analysis of {Brachypodium} hybridum and its diploid progenitors},
	volume = {11},
	issn = {2041-1723},
	doi = {10.1038/s41467-020-17302-5},
	abstract = {Our understanding of polyploid genome evolution is constrained because we cannot know the exact founders of a particular polyploid. To differentiate between founder effects and post polyploidization evolution, we use a pan-genomic approach to study the allotetraploid Brachypodium hybridum and its diploid progenitors. Comparative analysis suggests that most B. hybridum whole gene presence/absence variation is part of the standing variation in its diploid progenitors. Analysis of nuclear single nucleotide variants, plastomes and k-mers associated with retrotransposons reveals two independent origins for B. hybridum, {\textasciitilde}1.4 and {\textasciitilde}0.14 million years ago. Examination of gene expression in the younger B. hybridum lineage reveals no bias in overall subgenome expression. Our results are consistent with a gradual accumulation of genomic changes after polyploidization and a lack of subgenome expression dominance. Significantly, if we did not use a pan-genomic approach, we would grossly overestimate the number of genomic changes attributable to post polyploidization evolution.},
	language = {eng},
	number = {1},
	journal = {Nature Communications},
	author = {Gordon, Sean P. and Contreras-Moreira, Bruno and Levy, Joshua J. and Djamei, Armin and Czedik-Eysenberg, Angelika and Tartaglio, Virginia S. and Session, Adam and Martin, Joel and Cartwright, Amy and Katz, Andrew and Singan, Vasanth R. and Goltsman, Eugene and Barry, Kerrie and Dinh-Thi, Vinh Ha and Chalhoub, Boulos and Diaz-Perez, Antonio and Sancho, Ruben and Lusinska, Joanna and Wolny, Elzbieta and Nibau, Candida and Doonan, John H. and Mur, Luis A. J. and Plott, Chris and Jenkins, Jerry and Hazen, Samuel P. and Lee, Scott J. and Shu, Shengqiang and Goodstein, David and Rokhsar, Daniel and Schmutz, Jeremy and Hasterok, Robert and Catalan, Pilar and Vogel, John P.},
