@article{huning.aghakouchak_2018,
  title = {Mountain Snowpack Response to Different Levels of Warming},
  author = {Huning, Laurie S. and AghaKouchak, Amir},
  year = {2018},
  month = oct,
  journal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {115},
