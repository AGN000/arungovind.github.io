@article{hughes.etal_2015,
  title = {The Last Eurasian Ice Sheets {\textendash} a Chronological Database and Time-slice Reconstruction, DATED-1},
  author = {Hughes, Anna L. C. and Gyllencreutz, Richard and Lohne, {\O}ystein S. and Mangerud, Jan and Svendsen, John Inge},
