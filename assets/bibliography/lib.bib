@article{brenner.etal_1983,
  title = {Slope-Induced Errors in Radar Altimetry over Continental Ice Sheets},
  author = {Brenner, A. C. and Blndschadler, R. A. and Thomas, R. H. and Zwally, H. J.},
  year = {1983},
