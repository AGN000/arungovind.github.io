}

@article{lilleoren.etal_2012,
  title = {The Relative Age of Mountain Permafrost {\textemdash} Estimation of Holocene Permafrost Limits in Norway},
  author = {Lille{\o}ren, Karianne S. and Etzelm{\"u}ller, Bernd and Schuler, Thomas V. and Gisn{\aa}s, Kjersti and Humlum, Ole},
