@article{moudry.etal_2022,
  title = {Effects of Environmental Conditions on ICESat-2 Terrain and Canopy Heights Retrievals in Central European Mountains},
  author = {Moudr{\'y}, V{\'i}t{\v e}zslav and Gdulov{\'a}, Kate{\v r}ina and G{\'a}bor, Luk{\'a}{\v s} and {\v S}{\'a}rovcov{\'a}, Eli{\v s}ka and Bart{\'a}k, Vojt{\v e}ch and Leroy, Fran{\c c}ois and {\v S}patenkov{\'a}, Olga and Rocchini, Duccio and Pro{\v s}ek, Ji{\v r}{\'i}},
