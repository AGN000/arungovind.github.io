@article{huss.etal_2010,
  title = {Future High-Mountain Hydrology:  A New Parameterization of Glacier Retreat},
  shorttitle = {Future High-Mountain Hydrology},
  author = {Huss, M. and Jouvet, G. and Farinotti, D. and Bauder, A.},
