@article{sejrup.etal_2004,
  title = {Configuration, History and Impact of the Norwegian Channel Ice Stream},
  author = {Sejrup, Hans Petter and Larsen, Eiliv and Haflidason, Haflidi and Berstad, Ida M. and Hjelstuen, Berit O. and Jonsdottir, Hafdis E. and King, Edward L. and Landvik, Jon and Longva, Oddvar and Nyg{\aa}rd, Atle and Ottesen, Dag and Raunholm, St{\aa}le and Rise, Leif and Stalsberg, Knut},
  year = {2004},
  month = may,
  journal = {Boreas},
  volume = {32},
