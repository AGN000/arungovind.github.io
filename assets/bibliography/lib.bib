@article{fiddes.gruber_2014,
  title = {TopoSCALE v.1.0: Downscaling Gridded Climate Data in Complex Terrain},
  shorttitle = {TopoSCALE v.1.0},
  author = {Fiddes, J. and Gruber, S.},
  year = {2014},
