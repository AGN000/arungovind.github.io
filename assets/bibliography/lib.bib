@article{aas.etal_2017,
  title = {A Tiling Approach to Represent Subgrid Snow Variability in Coupled Land Surface{\textendash}Atmosphere Models},
  author = {Aas, Kjetil Schanke and Gisn{\aa}s, Kjersti and Westermann, Sebastian and Berntsen, Terje Koren},
  year = {2017},
