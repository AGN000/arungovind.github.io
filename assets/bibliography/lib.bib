}

@article{peng.etal_2010,
  title = {Change in Winter Snow Depth and Its Impacts on Vegetation in China},
  author = {PENG, {\relax SHUSHI} and PIAO, {\relax SHILONG} and CIAIS, {\relax PHILIPPE} and FANG, {\relax JINGYUN} and WANG, {\relax XUHUI}},
