@incollection{carey.etal_2021,
  title = {A Socio-Cryospheric Systems Approach to Glacier Hazards, Glacier Runoff Variability, and Climate Change},
  booktitle = {Snow and Ice-Related Hazards, Risks, and Disasters},
  author = {Carey, Mark and McDowell, Graham and Huggel, Christian and Marshall, Becca and Moulton, Holly and Portocarrero, C{\'e}sar and Provant, Zachary and Reynolds, John M. and Vicu{\~n}a, Luis},
  year = {2021},
