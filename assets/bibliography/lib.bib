@article{murcia.etal_2022,
  title = {Validation of European-Scale Simulated Wind Speed and Wind Generation Time Series},
  author = {Murcia, Juan Pablo and Koivisto, Matti Juhani and Luzia, Graziela and Olsen, Bjarke T. and Hahmann, Andrea N. and S{\o}rensen, Poul Ejnar and Als, Magnus},
  year = {2022},
