@article{zhou.etal_2021,
  title = {Inter-Comparison of Snow Depth over Arctic Sea Ice from Reanalysis Reconstructions and Satellite Retrieval},
  author = {Zhou, Lu and Stroeve, Julienne and Xu, Shiming and Petty, Alek and Tilling, Rachel and Winstrup, Mai and Rostosky, Philip and Lawrence, Isobel R. and Liston, Glen E. and Ridout, Andy and Tsamados, Michel and Nandan, Vishnu},
  year = {2021},
