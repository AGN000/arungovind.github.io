@article{wenau.etal_2022,
  title = {Derisking Offshore Windfarm Installation by Sub-Seafloor Boulder Detection Based on Dedicated Seismic Diffraction Imaging},
  author = {Wenau, Stefan and Schwarz, Benjamin and Bihler, Viola and Boyer, Eric and Preu, Benedict},
