  shorttitle = {Improving the Snowpack Monitoring in the Mountainous Areas of Sweden from Space},
  author = {Zhang, Jie and Pohjola, Veijo A and Pettersson, Rickard and Norell, Bj{\"o}rn and Marchand, Wolf-Dietrich and Clemenzi, Ilaria and Gustafsson, David},
