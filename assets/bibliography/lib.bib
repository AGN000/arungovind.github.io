}

@article{xu.etal_2021c,
  title = {Global Estimation and Assessment of Monthly Lake/Reservoir Water Level Changes Using ICESat-2 ATL13 Products},
  author = {Xu, Nan and Zheng, Huiying and Ma, Yue and Yang, Jian and Liu, Xinyuan and Wang, Xiaohua},
