@article{pirk.etal_2022,
  title = {Inferring Surface Energy Fluxes Using Drone Data Assimilation in Large Eddy Simulations},
  author = {Pirk, Norbert and Aalstad, Kristoffer and Westermann, Sebastian and Vatne, Astrid and {van Hove}, Alouette and Tallaksen, Lena Merete and Cassiani, Massimo and Katul, Gabriel},
