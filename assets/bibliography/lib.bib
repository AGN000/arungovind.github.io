@article{livneh.badger_2020,
  title = {Drought Less Predictable under Declining Future Snowpack},
  author = {Livneh, Ben and Badger, Andrew M.},
  year = {2020},
  month = may,
  journal = {Nat. Clim. Chang.},
  volume = {10},
  number = {5},
  pages = {452--458},
  issn = {1758-678X},
  doi = {10.1038/s41558-020-0754-8},
  urldate = {2023-02-01},
  abstract = {Mountain snowpack serves as an immense natural water reservoir, and knowledge of snow conditions helps predict seasonal water availability and offers critical early warning of hydrologic drought. This paradigm faces an impending challenge given consensus that a smaller fraction of future precipitation will fall as snow. Here, we apply downscaled hydrologic simulations from 28 climate model projections to show that by mid-century (2036{\textendash}2065), 69\% of historically snowmelt-dominated areas of the western United States see a decline in the ability of snow to predict seasonal drought, increasing to 83\% by late century (2070{\textendash}2099). Reduced predictability arises when peak snowpack approaches zero or because of decreased warm-season runoff efficiency. Changes in drought prediction skill show significant (P\,{$<$}\,0.01) elevation dependence, with lower-elevation coastal areas most impacted by warming. Ancillary predictive information can only partially mitigate snow-based predictability losses to 65\% of areas, underscoring the importance of declining future snowpack.},
  copyright = {‚≠ê‚≠ê‚≠ê},
  langid = {english},
