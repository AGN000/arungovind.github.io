@article{shen2018,
  title = {Full-Waveform Inversion: The next Leap Forward in Subsalt Imaging},
  shorttitle = {Full-Waveform Inversion},
  author = {Shen, Xukai and Ahmed, Imtiaz and Brenders, Andrew and Dellinger, Joe and Etgen, John and Michell, Scott},
