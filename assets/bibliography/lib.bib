@article{gisnas.etal_2016,
  title = {Small-Scale Variation of Snow in a Regional Permafrost Model},
  author = {Gisn{\aa}s, Kjersti and Westermann, Sebastian and Schuler, Thomas Vikhamar and Melvold, Kjetil and Etzelm{\"u}ller, Bernd},
  year = {2016},
  month = jun,
  journal = {The Cryosphere},
  volume = {10},
