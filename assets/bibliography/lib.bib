  author = {Nielsen, S{\o}ren B. and Gallagher, Kerry and Leighton, Callum and Balling, Niels and Svenningsen, Lasse and Jacobsen, Bo Holm and Thomsen, Erik and Nielsen, Ole B. and {Heilmann-Clausen}, Claus and Egholm, David L. and Summerfield, Michael A. and Clausen, Ole R{\o}n{\o} and Piotrowski, Jan A. and Thorsen, Marianne R. and Huuse, Mads and Abrahamsen, Niels and King, Chris and {Lykke-Andersen}, Holger},
  year = {2009},
