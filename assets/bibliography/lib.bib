  title = {Terrain-Guided Flatten Memory Network for Deep Spatial Wind Downscaling},
  author = {Yu, Tingzhao and Yang, Ruyi and Huang, Yan and Gao, Jinbing and Kuang, Qiuming},
  year = {2022},
  journal = {IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing},
  volume = {15},
