@article{gavas.etal_2023,
  title = {Spatial-SMOTE for Handling Imbalance in Spatial Regression Tasks},
  author = {Gavas, Rahul Dasharath and Das, Monidipa and Ghosh, Soumya Kanti and Pal, Arpan},
