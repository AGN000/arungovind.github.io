@article{xu.etal_2017,
  title = {Spatial{\textendash}Temporal Variability of Snow Cover and Depth in the Qinghai{\textendash}Tibetan Plateau},
  author = {Xu, Wenfang and Ma, Lijuan and Ma, Minna and Zhang, Haicheng and Yuan, Wenping},
