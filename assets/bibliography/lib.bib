  annotation = {üè∑Ô∏è /unread},
  file = {C:\Users\ZOZN109\Zotero\storage\5X9NP4SE\Vionnet et al_2021_Multi-scale snowdrift-permitting modelling of mountain snowpack.pdf}
}

@article{vogel.etal_2023,
  title = {An Evaluation Framework for Downscaling and Bias Correction in Climate Change Impact Studies},
