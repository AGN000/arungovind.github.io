@article{zhang.etal_2021a,
  title = {A Maximum Bathymetric Depth Model to Simulate Satellite Photon-Counting Lidar Performance},
  author = {Zhang, Wenhao and Xu, Nan and Ma, Yue and Yang, Bisheng and Zhang, Zhiyu and Wang, Xiao Hua and Li, Song},
