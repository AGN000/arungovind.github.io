@article{silva.etal_2021,
  title = {Fusing Simulated GEDI, ICESat-2 and NISAR Data for Regional Aboveground Biomass Mapping},
  author = {Silva, Carlos Alberto and Duncanson, Laura and Hancock, Steven and Neuenschwander, Amy and Thomas, Nathan and Hofton, Michelle and Fatoyinbo, Lola and Simard, Marc and Marshak, Charles Z. and Armston, John and Lutchke, Scott and Dubayah, Ralph},
  year = {2021},
