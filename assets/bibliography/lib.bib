@article{corbett.smith_2022,
  title = {A Study of a Miniature TDLAS System Onboard Two Unmanned Aircraft to Independently Quantify Methane Emissions from Oil and Gas Production Assets and Other Industrial Emitters},
  author = {Corbett, Abigail and Smith, Brendan},
  year = {2022},
