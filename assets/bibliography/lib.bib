@article{huning.aghakouchak_2020,
  title = {Global Snow Drought Hot Spots and Characteristics},
  author = {Huning, Laurie S. and AghaKouchak, Amir},
  year = {2020},
  month = aug,
  journal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {117},
