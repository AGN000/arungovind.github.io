@article{luthcke.etal_2021,
  title = {ICESat-2 Pointing Calibration and Geolocation Performance},
  author = {Luthcke, S. B. and Thomas, T. C. and Pennington, T. A. and Rebold, T. W. and Nicholas, J. B. and Rowlands, D. D. and Gardner, A. S. and Bae, S.},
