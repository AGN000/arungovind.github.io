  author = {Phipps, Steven J. and McGregor, Helen V. and Gergis, Jo{\"e}lle and Gallant, Ailie J. E. and Neukom, Raphael and Stevenson, Samantha and Ackerley, Duncan and Brown, Josephine R. and Fischer, Matt J. and {van Ommen}, Tas D.},
