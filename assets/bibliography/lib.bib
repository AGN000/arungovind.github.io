@misc{nguyen.etal_2023,
  title = {ClimaX: A Foundation Model for Weather and Climate},
  shorttitle = {ClimaX},
  author = {Nguyen, Tung and Brandstetter, Johannes and Kapoor, Ashish and Gupta, Jayesh K. and Grover, Aditya},
  year = {2023},
