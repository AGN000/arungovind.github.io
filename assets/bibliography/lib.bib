@article{anderson_1998,
  title = {Near-Surface Thermal Profiles in Alpine Bedrock: Implications for the Frost Weathering of Rock},
  shorttitle = {Near-Surface Thermal Profiles in Alpine Bedrock},
  author = {Anderson, Robert S.},
