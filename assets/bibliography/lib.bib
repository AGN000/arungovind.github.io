@article{grunewald.etal_2013,
  title = {Statistical Modelling of the Snow Depth Distribution in Open Alpine Terrain},
  author = {Gr{\"u}newald, T. and St{\"o}tter, J. and Pomeroy, J. W. and Dadic, R. and Moreno Ba{\~n}os, I. and Marturi{\`a}, J. and Spross, M. and Hopkinson, C. and Burlando, P. and Lehning, M.},
