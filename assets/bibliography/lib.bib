@article{regnell.etal_2019,
  title = {Tracing the Last Remnants of the Scandinavian Ice Sheet: Ice-Dammed Lakes and a Catastrophic Outburst Flood in Northern Sweden},
  shorttitle = {Tracing the Last Remnants of the Scandinavian Ice Sheet},
  author = {Regn{\'e}ll, Carl and Mangerud, Jan and Svendsen, John Inge},
