@article{xu.etal_2021b,
  title = {Surface-Water-Level Changes During 2003{\textendash}2019 in Australia Revealed by ICESat/ICESat-2 Altimetry and Landsat Imagery},
  author = {Xu, Nan and Ma, Yue and Zhang, Wenhao and Wang, Xiao Hua},
  year = {2021},
  month = jul,
  journal = {IEEE Geosci. Remote Sensing Lett.},
  volume = {18},
  number = {7},
