			<e:List id="mahjongList" x="80" anchorOffsetX="0" verticalCenter="0" itemRendererSkinName="Mj_CheckBox">
				<e:layout>
					<e:TileLayout orientation="rows" horizontalAlign="center" requestedRowCount="2" requestedColumnCount="3" verticalAlign="middle" rowAlign="justifyUsingHeight" paddingLeft="0" rowHeight="50"/>
				</e:layout>
			</e:List>
		</e:Group>
	</e:Scroller>
	<e:Scroller id="sc2" width="720" height="164" y="395" scrollPolicyH="off" scrollPolicyV="off" bounces="false" x="60">
