		<e:Image source="coin_match_choose_line" horizontalCenter="0" top="0"/>
		<e:RadioButton id="mahjongButton" label="" horizontalCenter="0" top="50" groupName="gameType">
			<e:skinName>
				<e:Skin states="up,down,disabled">
				<e:Image width="100%" height="100%" source="coin_match_choose_btnNoSelect" source.down="coin_match_choose_btnSelect"/>
	<e:Image source="coin_match_choose_majiangNoSelect" horizontalCenter="0" verticalCenter="0" source.down="coin_match_choose_majiangSelect"/>
				</e:Skin>
			</e:skinName>
		</e:RadioButton>
		<e:RadioButton id="pokerButton" label="" top="280" horizontalCenter="0" groupName="gameType">
			<e:skinName>
			<e:Skin states="up,down,disabled">
				<e:Image width="100%" height="100%" source="coin_match_choose_btnNoSelect" source.down="coin_match_choose_btnSelect"/>
	<e:Image source="coin_match_choose_pokerNoSelect" horizontalCenter="0" verticalCenter="0" source.down="coin_match_choose_pokerSelect"/>
			</e:Skin>
			</e:skinName>
		</e:RadioButton>
	</e:Group>
	<e:Button label="Button" x="622" y="634" visible="false"/>
</e:Skin>