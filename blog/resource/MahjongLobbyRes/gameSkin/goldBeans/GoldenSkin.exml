		<e:HorizontalLayout horizontalAlign="center" verticalAlign="middle" gap="60"/>
		</e:layout>
	</e:List>
	<e:Rect id="panelMask" left="0" right="0" top="0" bottom="0" fillColor="0x474646" fillAlpha="0.5" visible="false"/>
