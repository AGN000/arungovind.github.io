<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="GoldenSkin" width="1280" height="720" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" xmlns:tween="egret.tween.*">
