			<e:skinName>
				<e:Skin states="up,down,disabled" width="60" height="56">
				<e:Image source="coin_match_spriteBack" horizontalCenter="0" verticalCenter="0" scaleX.down="0.9" scaleY.down="0.9"/>
				</e:Skin>
			</e:skinName>
		</e:Button>
		<e:Label id="curGameName" text="" size="40" fontFamily="SimHei" textColor="0xffd68c" verticalCenter="0" left="130"/>
		<e:Button id="helpButton" label="" verticalCenter="0" right="30">
			<e:skinName>
				<e:Skin states="up,down,disabled" width="65" height="66">
				<e:Image source="coin_match_tips" horizontalCenter="0" verticalCenter="0" scaleX.down="0.9" scaleY.down="0.9"/>
				</e:Skin>
			</e:skinName>
		</e:Button>
		<e:Group id="moneyGroup" width="200" height="50" anchorOffsetX="0" anchorOffsetY="0" verticalCenter="0" right="130">
